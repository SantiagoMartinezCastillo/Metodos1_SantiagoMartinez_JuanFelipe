   "id": "c81853a5",
   "metadata": {},
   "outputs": [],
   "source": [
    "x=np.linspace(0.1,0.9,1000)\n",
    "def entropia(x_):\n",
    "    res = -1*(x*np.log(x)+(1-x)*np.log(1-x))\n",
    "    return res"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "6b87f57f",
   "metadata": {},
